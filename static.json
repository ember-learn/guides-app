{
  "https_only": true,
  "root": "dist",
  "error_page": "_empty.html",
  "force_redirect": true,
  "redirects": {
    "/": {
      "url": "https://$host/release/"
    },
    "/current/(.*)": {
      "url": "https://$host/release/$1"
    }
  },
  "headers": {
    "/**/*.html": {
      "Cache-Control": "public, max-age=3600"
    },
    "/content/**": {
      "Cache-Control": "public, max-age=3600"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000"
    }
  }
}
