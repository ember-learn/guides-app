{
  "https_only": true,
  "root": "dist",
  "error_page": "_empty.html",
  "force_redirect": true,
  "redirects": {
    "/": {
      "url": "/release/"
    },
    "/current/": {
      "url": "/release/"
    }
  },
  "headers": {
    "/*/**.html": {
      "Cache-Control": "public, max-age=3600"
    },
    "/content/**": {
      "Cache-Control": "public, max-age=3600"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000"
    }
  }
}
